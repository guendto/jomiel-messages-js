{
  "name": "jomiel-messages",
  "version": "0.2.0",
  "description": "The javascript bindings for the jomiel protobuf messages.",
  "keywords": [
    "jomiel",
    "protobuf",
    "messages"
  ],
  "repository": "github:guendto/jomiel-messages-js",
  "author": "Toni Gündoğdu",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "prepare": "./bin/gen-static -p src/jomiel-proto && npm run prettier",
    "prettier": "prettier-eslint --write bin/gen-static \"**/*.@(js|md|json|yaml)\"",
    "build-docs": "rimraf doc/ && jsdoc --pedantic -c ./jsdoc.json ."
  },
  "dependencies": {
    "protobufjs": "^6.11.2"
  },
  "devDependencies": {
    "chalk": "^4.1.1",
    "docopt": "^0.6.2",
    "execa": "^5.1.1",
    "glob": "^7.1.7",
    "jsdoc": "^3.6.7",
    "minimist": "^1.2.5",
    "prettier-eslint-cli": "^5.0.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "uglify-js": "^3.13.9"
  }
}
